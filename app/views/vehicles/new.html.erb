<h1>Add your vehicle</h1>
<div class="container">
  <%= simple_form_for(@vehicle) do |f| %>
    <%= f.input :make %>
    <%= f.input :model %>
    <%= f.input :year %>
    <%= f.input :number_of_passengers %>
    <%= f.input :transmission %>
    <%= f.input :price_per_day %>
    <%= f.input :location %>
    <%= f.input :description %>
    <%= f.submit %>
  <% end %>
</div>

<%= link_to "Back to all Vehicles", vehicles_path %>
